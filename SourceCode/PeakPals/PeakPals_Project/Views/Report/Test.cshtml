@{
    var testName = ViewBag.TestName;
}



<body id="report-body">
    <div class="text-center" id="test-details-div" data-test="@testName">
        <h1 class="display-5 text-white welcome" style="margin-bottom: 0.5em;">@testName details</h1>
        <a href="/Report/Report" style="text-decoration: none; color: white;" class="btn btn-lg btn-danger my-3">Return to Analysis Page</a>
        <div class="container px-5" style="margin-bottom: 3em;" id="details-container">
            <div class="row" style="margin-left: auto; margin-right: auto;">
                <div class="col col-sm-12 col-md-6 col-lg-6 col-xl-6">
                    <div class="history-graph-div" id="@testName-test-graph"
                        style="width: 100%; height: 100%; margin-left: auto; margin-right: auto;"></div>
                </div>
                <div class="col col-sm-12 col-md-6 col-lg-6 col-xl-6 ">
                    <div class="history-table-div" id="@testName-test-table" style="height: 600px; overflow-y: auto !important;"></div>
                </div>
            </div>
        </div>
    </div>
</body>

@section Scripts {
    <script src="~/js/report.js"></script>
}
@section Styles {
    <link rel="stylesheet" href="~/css/report.css">
}